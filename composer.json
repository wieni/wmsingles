{
    "name": "wieni/wmsingles",
    "type": "drupal-module",
    "description": "Singles are used for one-off bundles eg a bundle with exactly ONE entity",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "drupal/core": "^8.0",
        "drupal/hook_event_dispatcher": "^1.19"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.0"
    },
    "repositories": {
        "drupal-composer": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php"
        ]
    }
}
